---
import Layout from '../../layouts/Layout.astro';
import AppLayout from '../../components/AppLayout';
import EventFormWrapper from '../../components/EventFormWrapper';

const title = 'Event Form';
const description = 'Create or edit an event';
---

<Layout title={title} description={description}>
  <AppLayout client:only="react" currentPath="/app/events">
    <EventFormWrapper client:only="react" />
  </AppLayout>
</Layout>
